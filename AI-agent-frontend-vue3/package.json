    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.6",
    "vite-plugin-windicss": "^1.9.4",
    "vue-tsc": "^3.0.7",
